{
  "name": "material-dashboard-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "eject": "npm run eject:smui-theme && npm run eject:sass",
    "eject:sass": "sass --style=compressed src/assets/scss:static/css",
    "eject:smui-theme": "smui-theme compile static/css/smui.css -i src/theme"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@smui-extra/accordion": "^6.0.0-beta.13",
    "@smui/button": "^6.0.0-beta.13",
    "@smui/card": "^6.0.0-beta.13",
    "@smui/common": "^6.0.0-beta.13",
    "@smui/drawer": "^6.0.0-beta.13",
    "@smui/icon-button": "^6.0.0-beta.13",
    "@smui/list": "^6.0.0-beta.13",
    "@smui/paper": "^6.0.0-beta.13",
    "@smui/snackbar": "^6.0.0-beta.13",
    "@smui/textfield": "^6.0.0-beta.13",
    "@smui/top-app-bar": "^6.0.0-beta.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.7",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module"
}
